{
  "name": "@labforward/markdown-parser",
  "version": "1.0.0",
  "description": "Extended Markdown parser",
  "homepage": "https://github.com/labforward/laboperator-markdown-parser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labforward/laboperator-markdown-parser.git"
  },
  "license": "Apache-2.0",
  "author": "labforward GmbH",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc-alias",
    "ci:esm": "node ./bin/test",
    "ci:test": "NODE_OPTIONS=--experimental-vm-modules jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "lint": "eslint --fix 'src/**/*.{ts,js}'",
    "prepare": "husky",
    "sort": "sort-package-json ./package.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "dependencies": {
    "hast-util-sanitize": "^5.0.1",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-to-hast": "^13.1.0",
    "micromark-core-commonmark": "^2.0.0",
    "micromark-factory-space": "^2.0.0",
    "micromark-util-character": "^2.1.0",
    "micromark-util-symbol": "^2.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@labforward/config": "~0.1.5",
    "@labforward/eslint-config-node": "~0.1.5",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.17",
    "eslint": "^8.56.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "license-checker-rseidelsohn": "^4.3.0",
    "lint-staged": "^15.2.2",
    "micromark-util-types": "^2.0.0",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2"
  },
  "packageManager": "yarn@4.0.1"
}
