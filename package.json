{
  "name": "@labforward/markdown-parser",
  "version": "1.0.0",
  "description": "Extended Markdown parser",
  "homepage": "https://github.com/labforward/laboperator-markdown-parser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labforward/laboperator-markdown-parser.git"
  },
  "license": "Apache-2.0",
  "author": "labforward GmbH",
  "type": "module",
  "exports": {
    ".": {
      "require": null,
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && tsc-alias",
    "ci:esm": "node ./bin/test",
    "ci:test": "NODE_OPTIONS=--experimental-vm-modules jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "lint": "eslint --fix 'src/**/*.{ts,js}'",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "sort": "sort-package-json ./package.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "dependencies": {
    "hast-util-sanitize": "^4.1.0",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^1.3.1",
    "mdast-util-to-hast": "^12.3.0",
    "micromark-core-commonmark": "^1.1.0",
    "micromark-factory-space": "^1.1.0",
    "micromark-util-character": "^1.2.0",
    "micromark-util-symbol": "^1.1.0",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.6",
    "@labforward/config": "~0.1.3",
    "@labforward/eslint-config-node": "~0.1.3",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.1",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "jest-junit": "^16.0.0",
    "license-checker-rseidelsohn": "^4.2.6",
    "lint-staged": "^13.2.3",
    "micromark-util-types": "^1.1.0",
    "prettier": "^2.8.8",
    "sort-package-json": "^2.5.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@1.22.19"
}
