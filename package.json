{
  "name": "@labforward/markdown-parser",
  "version": "1.0.0",
  "description": "Extended Markdown parser",
  "homepage": "https://github.com/labforward/laboperator-markdown-parser#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/labforward/laboperator-markdown-parser.git"
  },
  "license": "Apache-2.0",
  "author": "labforward GmbH",
  "type": "module",
  "exports": {
    ".": {
      "require": null,
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "del-cli dist && tsc -p tsconfig.json",
    "ci:esm": "node ./bin/test",
    "ci:test": "NODE_OPTIONS=--experimental-vm-modules jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "lint": "eslint --fix 'src/**/*.{ts,js}'",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "sort": "sort-package-json ./package.json",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand"
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "dependencies": {
    "hast-util-sanitize": "^4.1.0",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-to-hast": "^12.3.0",
    "micromark-core-commonmark": "^1.0.6",
    "micromark-factory-space": "^1.0.0",
    "micromark-util-character": "^1.1.0",
    "micromark-util-symbol": "^1.0.1",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@labforward/config": "~0.1.1",
    "@labforward/eslint-config-node": "~0.1.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.11",
    "del-cli": "^5.0.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "license-checker-rseidelsohn": "^4.2.5",
    "lint-staged": "^13.2.1",
    "micromark-util-types": "^1.0.2",
    "prettier": "^2.8.7",
    "sort-package-json": "^2.4.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@1.22.19",
  "importSort": {
    ".js, .ts": {
      "parser": "typescript",
      "style": "@labforward/config/import-sort"
    }
  }
}
